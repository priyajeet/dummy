{
    "presets": [
        [
            "es2015", { "modules": false }
        ],
        "es2016",
        "react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-export-extensions"
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "istanbul", {
                        "exclude": [
                            "**/*-test.js"
                        ]
                    }
                ],
                "rewire"
            ]
        }
    }
}
